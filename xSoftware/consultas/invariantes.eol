//1. No existen entidades con el mismo nombre
operation entidadesRepetidas():Boolean{
	
	var response = true;
	
	for(e in Entidad.allInstances){
		var count = 0;
		for(e2 in Entidad.allInstances){
			if(e=e2){
				count = count + 1;
			}
		}
		if(count > 1){
			response = false;
		}
	}
	
	return response;
}

//2. Todos los par�metros (path y query) de un servicio dado tienen un nombre �nico en el servicio.
operation Servicio parametrosRepetidos():Boolean{
	
	var response = true;
	
	for(parametro in Request.allInstances.select(r|r.servicioRest=self).parametros.select(p|p.tipo= "Path" or p.tipo="Query")){
		var count = 0;
		for(parametro2 in Request.allInstances.select(r|r.servicioRest=self).parametros.select(p|p.tipo= "Path" or p.tipo="Query")){
			if(parametro=parametro2){
				count = count + 1;
			}
		}
		if(count > 1){
			response = false;
		}
	}
	
	return response;
}

//3. Los servicios de tipo get no tienen body en el request.
operation ServiciosGetSinBody():Boolean{
	var response = true;
	var count = 0;
	for(request in Request.allInstances.select(rq|rq.servicioRest.metodo = "GET")) {
		if(request.tipo = "Body"){
			count = count + 1;
		}
	}
	if(count > 1){
		response = false;
	}
	return response;
}

